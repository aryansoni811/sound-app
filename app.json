{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "simplemeditation",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/cg-logo.png",
      "imagewidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.dantewada.heritageApp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDQJpIKq6-4rDSMyT9ODdURnJcmmic141Q"
        },
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      }
      // "accessFineLocation": true,
      // "accessCoarseLocation": true,
      // "foregroundService": {
      //   "notificationTitle": "Location Services",
      //   "notificationBody": "This app is accessing your location."
      // }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-location"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a8b81ae0-be0d-402f-aaa5-8f75bdd99420"
      },
      "expo-location": {
        "locationAlwaysAndWhenInUsePermission": "Allow Dantewada Heritage App to use your location."
      }
    }
  }
}
